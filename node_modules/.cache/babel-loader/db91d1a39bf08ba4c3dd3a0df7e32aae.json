AA0BA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,oBACA,IADA,EACA;AACA,aAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,CAAA;AACA,KAHA;AAIA,IAAA,UAJA,sBAIA,IAJA,EAIA;AACA,aAAA,CAAA,IAAA,CAAA;AACA,KANA;AAOA,IAAA,SAPA,qBAOA,EAPA,EAOA;AACA,aAAA,EAAA,IAAA,CAAA;AACA,KATA;AAUA,IAAA,OAVA,mBAUA,EAVA,EAUA;AACA;AAEA,UAAA,KAAA,SAAA,CAAA,EAAA,EAAA,GAAA,IAAA,CAAA,EAAA;AACA,aAAA,SAAA,CAAA,EAAA,EAAA,GAAA,IAAA,CAAA,CADA,CAEA;;AACA,aAAA,IAAA,CAAA,KAAA,CAAA,aAAA,EAAA,KAAA,SAAA,CAAA,EAAA,CAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,eAAA;AACA;AACA,KAnBA;AAoBA,IAAA,UApBA,sBAoBA,EApBA,EAoBA;AACA;AAEA,UAAA,KAAA,SAAA,CAAA,EAAA,EAAA,GAAA,IAAA,CAAA,EAAA;AACA,aAAA,SAAA,CAAA,EAAA,EAAA,GAAA,IAAA,CAAA,CADA,CAEA;;AACA,aAAA,IAAA,CAAA,KAAA,CAAA,kBAAA,EAAA,KAAA,SAAA,CAAA,EAAA,CAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,kBAAA;AACA;AACA;AA7BA;AA1BA,CAAA","sourcesContent":["<template>\r\n<div class=\"container vertical-scrollable\">\r\n\r\n  <b-modal \r\n    id=\"choose-detail\"\r\n    scrollable\r\n    title=\"Card-Detail\"\r\n    :data=\"modalData\">\r\n    <div class=\"card-container\">\r\n      <img :src=\"modalData === null ? '../PM_Back.jpg' : modalData.detail.img_url\" />\r\n      <div class=\"card-attr\">\r\n        <!--<b-table striped hover :items=\"modalData === null ? [] : addInArray(modalData.detail)\"></b-table>-->\r\n\r\n        <table class=\"table table-striped\">\r\n          <tbody>\r\n            <tr\r\n              v-for=\"(attr, index) in Object.entries(modalData.detail)\"\r\n              :key=\"attr.card_number\"\r\n              >\r\n              <th v-if=\"showTable(index)\">{{attr[0]}}</th>\r\n              <td \r\n                class=\"text-dark\"\r\n                v-if=\"showTable(index)\"\r\n                >\r\n                {{attr[1]}}\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </b-modal>\r\n\r\n  <div class=\"row\">\r\n    <div\r\n      class=\"card col-4\"\r\n      v-for=\"(element, index) in card_list\"\r\n      :key=\"element.detail.card_number\">\r\n\r\n      <img\r\n        :src=\"element.detail.img_url\"\r\n        title=\"Click to check out detail\"\r\n        v-b-modal.choose-detail\r\n        class=\"card-img-top\"\r\n        @click=\"modalData = element\"\r\n        alt=\"error\">\r\n\r\n      <div class=\"card-body d-flex flex-column\">\r\n        <h6 class=\"card-title\">{{element.detail.card_number}} {{element.detail.card_name}}</h6>\r\n        <p class=\"card-text\">{{element.detail.type}}</p>\r\n        <p>\r\n          <strong> Source: {{element.detail.source}} </strong>\r\n          <br>\r\n          <strong> Cost: {{element.detail.cost}} </strong>\r\n          <br>\r\n          <strong v-if=\"notValue(element.detail.AP)\"> AP: {{element.detail.AP}}</strong>\r\n          <br>\r\n          <strong v-if=\"notValue(element.detail.DP)\"> DP: {{element.detail.DP}}</strong>\r\n        </p>\r\n        \r\n        <div class=\"btn-group mt-auto\">\r\n          <b-button\r\n            class=\"btn-line\"\r\n            variant=\"outline-danger\"\r\n            @click=\"addCard(index)\"\r\n            >\r\n            +\r\n          </b-button>\r\n          <b-button\r\n            class=\"btn-line\"\r\n            variant=\"outline-danger\"\r\n            @click=\"removeCard(index)\"\r\n            >\r\n            -\r\n          </b-button>\r\n          <p class=\"my-auto float-right\">&nbsp;&nbsp;{{element.cnt}}/4</p>\r\n        </div>\r\n      </div>\r\n\r\n    </div>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n//import HelloWorld from \"./components/HelloWorld.vue\";\r\nimport 'bootstrap/dist/css/bootstrap.css';\r\nimport 'bootstrap-vue/dist/bootstrap-vue.css';\r\n\r\n\r\nexport default {\r\n  name: \"CardColumn\",\r\n  components: {\r\n    \r\n  },\r\n  data() {\r\n    const mydata = require(\"../../board/data.json\");\r\n    \r\n    const tester = mydata.map((detail, index) => {\r\n      return {detail, order: index + 1, excost: 0, exsource: 0, exap: 0, exdp: 0, overlap: [], cnt: 0, coin: 0};\r\n    });\r\n    console.log(tester);\r\n    \r\n    //const counter = [];\r\n\r\n    //for(let i = 0;i < tester.length;++i){\r\n    //  counter.push(0);\r\n    //}\r\n\r\n    return {\r\n      card_list: tester,\r\n      drag: false,\r\n      modalData: tester[0],\r\n      //card_count: counter,\r\n    };\r\n  },\r\n  methods: {\r\n    notValue(data) {\r\n      return !isNaN(parseInt(data));\r\n    },\r\n    addInArray(data) {\r\n      return [data];\r\n    },\r\n    showTable(id) {\r\n      return (id != 0);\r\n    },\r\n    addCard(id) {\r\n      //let pre_count = this.card_count[id];\r\n\r\n      if(this.card_list[id].cnt != 4){\r\n        this.card_list[id].cnt += 1;\r\n        //this.card_count.splice(id,1,this.card_list[id].cnt);\r\n        this.$bus.$emit(\"add-to-deck\",this.card_list[id]);\r\n        console.log(\"emit add card\");\r\n      }\r\n    },\r\n    removeCard(id) {\r\n      //let pre_count = this.card_count[id];\r\n\r\n      if(this.card_list[id].cnt != 0){\r\n        this.card_list[id].cnt -= 1;\r\n        //this.card_count.splice(id,1,this.card_list.cnt);\r\n        this.$bus.$emit(\"remove-from-deck\",this.card_list[id]);\r\n        console.log(\"emit remove card\");\r\n      }\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.vertical-scrollable> .row {\r\n  positoin:  absolute;\r\n  overflow-y: scroll;\r\n  max-height: 90vh;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/index/components"}]}