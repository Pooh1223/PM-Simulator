UAAA,GAAA,CAAA,KAAA,UAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA;AARA,GAxBA;AAkCA,EAAA,QAAA,EAAA,EAlCA;AAoCA,EAAA,OApCA,qBAoCA;AAAA;;AACA,SAAA,IAAA,CAAA,GAAA,CAAA,gBAAA,EAAA,UAAA,EAAA,EAAA,IAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,cAAA,IAAA;;AACA,MAAA,KAAA,CAAA,SAAA,CAAA,MAAA,CAAA,EAAA,EAAA,CAAA,EAAA,IAAA;AACA,KAHA;AAKA,SAAA,IAAA,CAAA,GAAA,CAAA,mBAAA,EAAA,UAAA,EAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,YAAA,EAAA,GAAA,UAAA;;AACA,MAAA,KAAA,CAAA,SAAA,CAAA,MAAA,CAAA,EAAA,EAAA,CAAA;AACA,KAHA;AAIA;AA9CA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n      <div class=\"ex-cards\"\r\n        @click=\"openTemp\">\r\n        <img src=\"../PM_Back.jpg\" />\r\n      </div>\r\n\r\n      <b-button\r\n        id=\"ex-deck-show\"\r\n        class=\"btn-block\"\r\n        variant=\"success\"\r\n        @click=\"openTemp\"\r\n        >\r\n        Show all\r\n      </b-button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport 'bootstrap/dist/css/bootstrap.css';\r\n//import DialogDrag from 'vue-dialog-drag';\r\n\r\nexport default {\r\n  name: \"exdeck\",\r\n  display: \"ExDeck\",\r\n  order: 6,\r\n  components: {\r\n    //DialogDrag,\r\n  },\r\n  data() {\r\n    const message = [];\r\n\r\n    for(let i = 0;i < 15;++i){\r\n      message.push(toString(i + 1));\r\n    }\r\n    \r\n    const mapped_list = message.map((name, index) => {\r\n      return { name, order: index + 1 };\r\n    });\r\n\r\n    return {\r\n      card_list: mapped_list,\r\n      showDialog: false,\r\n      preText: 2,\r\n    };\r\n  },\r\n  methods: {\r\n    openTemp() {\r\n      this.$bus.$emit(\"open-from-ex-deck\",\"Ex-Deck\",this.card_list);\r\n      console.log(\"ex-deck: sent!\");\r\n    },\r\n    toggleDialog(data) {\r\n      this.showDialog = !this.showDialog;\r\n      console.log(data);\r\n    },\r\n  },\r\n  computed: {\r\n  },\r\n  mounted() {\r\n    this.$bus.$on(\"add-to-ex-deck\",(id,card) => {\r\n      console.log(\"add card:\" + card);\r\n      this.card_list.splice(id,0,card);\r\n    });\r\n\r\n    this.$bus.$on(\"remove-to-ex-deck\",(id) => {\r\n      console.log(\"remove \" + id + \"-th card\");\r\n      this.card_list.splice(id,1);\r\n    });\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n.ex-cards {\r\n  cursor: click;\r\n  float: left;\r\n  //width: 50%;\r\n  //height: 300px;\r\n  background-image: url(\"../PM_Back.jpg\");\r\n  background-size: 100%;\r\n  background-repeat: no-repeat;\r\n  //background-position: center;\r\n  padding-bottom: 5px;\r\n}\r\n.ex-cards img {\r\n  vertical-align: top;\r\n  max-width: 100%;\r\n  opacity: 0;\r\n}\r\n#ex-deck-show {\r\n  padding-bottom: 10px;\r\n  text-align: center;\r\n}\r\n\r\n</style>\r\n\r\n<style src=\"vue-dialog-drag/dist/vue-dialog-drag.css\"></style>\r\n<style src=\"vue-dialog-drag/dist/vue-drop-area.css\"></style>\r\n\r\n<!-- optional dialog styles, see example -->\r\n<style src=\"vue-dialog-drag/dist/dialog-styles.css\"></style>"],"sourceRoot":"src/components"}]}