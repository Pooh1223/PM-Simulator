 test id: {{from_which_card}}</p>\r\n\r\n      <draggable\r\n          class=\"overlap-card-list\"\r\n          tag=\"div\"\r\n          v-model=\"sublist\"\r\n          v-bind=\"dragOptions\"\r\n          @start.stop=\"drag = true\"\r\n          @end.stop=\"dismissPinned\"\r\n        >\r\n          <transition-group\r\n            class=\"row\"\r\n            type=\"transition\"\r\n            :name=\"!drag ? 'flip-list' : null\"\r\n          >\r\n            \r\n            <div\r\n              :class = \"index % 5 == 0 ? 'item col-2 offset-1' : 'item col-2' \"\r\n              v-for=\"(element, index) in sublist\"\r\n              :key=\"element.order\"\r\n              v-b-modal.card-detail\r\n              @mousedown=\"firePinned\"\r\n            >\r\n              <img \r\n                @click=\"openModal(element.order)\"\r\n                src=\"../PM_Back.jpg\" />\r\n              <i\r\n                :class=\"\r\n                  element.fixed ? 'fa fa-anchor' : 'glyphicon glyphicon-pushpin'\r\n                \"\r\n                @click=\"element.fixed = !element.fixed\"\r\n                aria-hidden=\"true\"\r\n              >{{index}} , {{element.order}}</i>\r\n            </div>\r\n          </transition-group>\r\n        </draggable>\r\n\r\n    </dialog-drag>\r\n\r\n    <b-modal \r\n      id=\"card-detail\"\r\n      scrollable\r\n      title=\"Card-Detail\"\r\n      hide-backdrop\r\n      :data=\"modalData\">\r\n      \r\n        <img src=\"../PM_Back.jpg\" />\r\n        <div class=\"card-attr\">\r\n          <p class=\"my-2\">\r\n            Test ,Test {{modalData}}\r\n          </p>\r\n        </div>\r\n\r\n        <draggable\r\n          class=\"overlap-card-list\"\r\n          tag=\"div\"\r\n          v-model=\"list\"\r\n          v-bind=\"dragOptions\"\r\n          @start.stop=\"drag = true\"\r\n          @end.stop=\"dismissPinned\"\r\n          @move=\"mmove\"\r\n          @drag-start=\"dragStart\"\r\n          @drag-end=\"dragEnd\"\r\n        >\r\n          <transition-group\r\n            class=\"row\"\r\n            type=\"transition\"\r\n            :name=\"!drag ? 'flip-list' : null\"\r\n          >\r\n            \r\n            <div\r\n              :class = \"index % 5 == 0 ? 'item col-2 offset-1' : 'item col-2' \"\r\n              v-for=\"(element, index) in list\"\r\n              :key=\"element.order\"\r\n              v-b-modal.card-detail\r\n              @mousedown=\"firePinned\"\r\n            >\r\n              <img \r\n                @click=\"openModal(element.order)\"\r\n                src=\"../PM_Back.jpg\" />\r\n              <i\r\n                :class=\"\r\n                  element.fixed ? 'fa fa-anchor' : 'glyphicon glyphicon-pushpin'\r\n                \"\r\n                @click=\"element.fixed = !element.fixed\"\r\n                aria-hidden=\"true\"\r\n              >{{index}} , {{element.order}}</i>\r\n            </div>\r\n          </transition-group>\r\n        </draggable>\r\n      \r\n    </b-modal>\r\n\r\n    <draggable\r\n      class=\"card-list\"\r\n      tag=\"div\"\r\n      v-model=\"list\"\r\n      v-bind=\"dragOptions\"\r\n      @start=\"drag = true\"\r\n      @end=\"drag = false\"\r\n    >\r\n      <transition-group\r\n        class=\"row\"\r\n        type=\"transition\"\r\n        :name=\"!drag ? 'flip-list' : null\"\r\n      >\r\n        \r\n        <div\r\n          :class = \"index % 5 == 0 ? 'item col-2 offset-1' : 'item col-2' \"\r\n          v-for=\"(element, index) in list\"\r\n          :key=\"'ma-' + index\"\r\n        >\r\n          <img\r\n            v-b-modal.card-detail\r\n            @click=\"openModal(element.order)\"\r\n            src=\"../PM_Back.jpg\" />\r\n          <i\r\n            :class=\"\r\n              element.fixed ? 'fa fa-anchor' : 'glyphicon glyphicon-pushpin'\r\n            \"\r\n            @click=\"element.fixed = !element.fixed\"\r\n            aria-hidden=\"true\"\r\n          >{{index}} , {{element.order}}</i>\r\n\r\n          <div class=\"text-center\">\r\n            <b-button\r\n              \r\n              id=\"overlap\"\r\n              variant=\"outline-primary\"\r\n              @click=\"openDialog\">\r\n              Overlap\r\n            </b-button>\r\n          </div>\r\n        </div>\r\n      </transition-group>\r\n    </draggable>  \r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport draggable from \"vuedraggable\";\r\nimport DialogDrag from 'vue-dialog-drag';\r\n//import DropArea from 'vue-dialog-drag/dist/drop-area';\r\n//import 'bootstrap/dist/css/bootstrap.css';\r\nimport 'bootstrap-vue/dist/bootstrap-vue.css';\r\n\r\n\r\nconst message = [\"1\", \"2\", \"3\", \"4\", \"5\", \"6\", \"7\", \"8\"];\r\nconst inMes = [\"1\", \"2\", \"3\", \"4\", \"5\", \"6\"];\r\n\r\nexport default {\r\n  name: \"main-area\",\r\n  display: \"main-area\",\r\n  order: 6,\r\n  components: {\r\n    draggable,\r\n    DialogDrag,\r\n    //DropArea,\r\n  },\r\n  data() {\r\n    const rowofcard = 5;\r\n    const mapped_list = message.map((name, index) => {\r\n      return { name, order: index + 1 };\r\n    });\r\n\r\n    const mapped_sublist = inMes.map((name, index) => {\r\n      return { name, order: index + 1 };\r\n    });\r\n\r\n    return {\r\n      row_of_card: rowofcard,\r\n      list: mapped_list,\r\n      sublist: mapped_sublist,\r\n      drag: false,\r\n      modalData: null,\r\n      showDialog: false,\r\n      from_which_card: null,\r\n      pinDialog: false,\r\n    };\r\n  },\r\n  methods: {\r\n    openModal(data) {\r\n      console.log(\"jizz:\" + data);\r\n      this.modalData = data;\r\n      console.log(\"pinned when modal open:\" + this.pinDialog);\r\n    },\r\n    openDialog (data) {\r\n      this.showDialog = true;\r\n      this.$from_which_card = data;\r\n      console.log(\"it should open!\");\r\n      console.log(this.list);\r\n    },\r\n    closeDialog () {\r\n      this.showDialog = false;\r\n      console.log(\"it should close!\");\r\n    },\r\n    moveCard () {\r\n      this.drag = true;\r\n      console.log('click card on!');\r\n    },\r\n    occur () {\r\n      console.log('fire');\r\n    },\r\n    firePinned () {\r\n      this.pinDialog = true;\r\n      this.drag = true;\r\n      console.log(\"lock dialog while moving cards!\");\r\n    },\r\n    dismissPinned () {\r\n      \r\n      this.drag = false;\r\n      console.log(\"unlock dialog while moving is finished\");\r\n\r\n      this.$nextTick(() => {\r\n        this.pinDialog = false;\r\n        console.log(\"pinned:\" + this.pinDialog);\r\n      });\r\n    },\r\n    mmove () {\r\n      console.log(\"moving dialog\");\r\n    },\r\n    dragStart () {\r\n      console.log(\"start dragging dialog\");\r\n    },\r\n    dragEnd () {\r\n      this.pinDialog = false;\r\n      console.log(\"end of dragging dialog\");\r\n    }\r\n  },\r\n  computed: {\r\n    dragOptions() {\r\n      return {\r\n        animation: 200,\r\n        group: \"description\",\r\n        disabled: false,\r\n        ghostClass: \"ghost\",\r\n      };\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.button {\r\n  margin-top: 35px;\r\n}\r\n.flip-list-move {\r\n  transition: transform 0.5s;\r\n}\r\n.no-move {\r\n  transition: transform 0s;\r\n}\r\n.ghost {\r\n  opacity: 0.5;\r\n  background: #c8ebfb;\r\n}\r\n.card-list {\r\n  min-height: 20px;\r\n  max-width: 100%;\r\n}\r\n.item {\r\n  cursor: move;\r\n  float: left;\r\n  //width: 50%;\r\n  //height: 300px;\r\n  background-image: url(\"../PM_Back.jpg\");\r\n  background-size: 100%;\r\n  background-repeat: no-repeat;\r\n  //background-position: center;\r\n  padding: 0;\r\n}\r\n.item i {\r\n  cursor: pointer;\r\n}\r\n.item img {\r\n  vertical-align: top;\r\n  max-width: 100%;\r\n  opacity: 0;\r\n}\r\n#overlap {\r\n  margin-bottom: 5px;\r\n}\r\n.card-container {\r\n  overflow-y: auto;\r\n}\r\n.card-container img {\r\n  vertical-align: top;\r\n  max-width: 100%;\r\n}\r\n.overlap-card-list {\r\n  min-height: 20px;\r\n  max-width: 100%;\r\n  overflow-y: auto;\r\n}\r\n</style>\r\n\r\n<style src=\"vue-dialog-drag/dist/vue-dialog-drag.css\"></style>\r\n<style src=\"vue-dialog-drag/dist/vue-drop-area.css\"></style>\r\n\r\n<!-- optional dialog styles, see example -->\r\n<style src=\"vue-dialog-drag/dist/dialog-styles.css\"></style>"],"sourceRoot":"src/components"}]}